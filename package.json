{
  "name": "team_project",
  "version": "0.0.0",
  "description": "TeamProject",
  "type": "commonjs",
  "engines": {
    "node": "14.x"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p .",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production node ./dist/bin/server.js",
    "start:dev": "ts-node-dev --respawn --transpile-only src/bin/server.ts",
    "lint": "eslint --no-cache --ext .ts .",
    "lint:fix": "eslint --no-cache --ext .ts . --fix",
    "prebuild": "npm run swagger",
    "swagger": "tsoa spec",
    "predev": "npm run swagger",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\""
  },
  "author": "VolKor",
  "license": "ISC",
  "devDependencies": {
    "@types/swagger-ui-express": "^4.1.2",
    "concurrently": "^6.0.1",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-json-es": "^1.5.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.1",
    "ts-node-dev": "^1.1.6"
  },
  "dependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/passport": "^1.0.6",
    "@types/passport-jwt": "^3.0.5",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cross-env": "7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.3.0",
    "mongoose": "^5.12.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "prettier": "^2.2.1",
    "query-string": "^7.0.0",
    "swagger-ui-express": "^4.1.6",
    "tsoa": "^3.6.1",
    "typescript": "^4.2.3"
  }
}
